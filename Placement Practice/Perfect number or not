#include <stdio.h>
int main()
{
    int num;
    scanf("%d",&num);
    int temp=num;
    int res=0;
    for(int i=1;i<num;i++){
        if(num%i==0){
            res=res+i;
        }
    }
    if(res==temp){
        printf("Perfect number");
    }
    else{
        printf("Not a perfect number");
    }
    return 0;
}
