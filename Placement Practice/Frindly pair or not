#include <stdio.h>
int main()
{
    int num1,num2;
    scanf("%d %d",&num1,&num2);
    int sum1=0,sum2=0;
    for(int i=1;i<num1;i++){
        if(num1%i==0){
            sum1=sum1+i;
        }
    }
    for(int j=1;j<num2;j++){
        if(num2%j==0){
            sum2=sum2+j;
            }
    }
    printf("%d %d",sum1,sum2);
    if(sum1==num1 && sum2==num2){
        printf("Friendly pair");
    }
    else{
        printf("Not a frindly pair");
    }
    return 0;
}
