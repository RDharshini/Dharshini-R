#include <stdio.h>
int main()
{
   int n,m;
   scanf("%d %d",&n,&m);
   int arr1[n],arr2[m];
   for(int i=0;i<n;i++){
       scanf("%d",&arr1[i]);
   }
   for(int j=0;j<m;j++){
       scanf("%d",&arr2[j]);
   }
   int flag=0;
   for(int k=0;k<n;k++){
       if(arr1[k]!=arr2[k]){
           flag++;
       }
   }
   if(flag==0){
       printf("Same");
   }
   else{
       printf("Not Same");
   }
    return 0;
}
